<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>

    <link rel="stylesheet" type="text/css" href="login.css">
</head>
<body>

<!-- Header -->
<%- include('../partials/header'); %>

<!-- main section -->
<div class="wrapper">
        
    <div class="nested"> 
        
        <div class="boxBackground">
            <div class="form">
                <h2 class="textLogin">LOGIN</h2> 
        
                <form method="post" action="/loginAccount" id="login">
        
                 <p class="elt">
                    <input type="text" name="email" placeholder="Email" form="login">
                </p>
            <!--
                <p class="elt">
                    <input type="text" name="userName" placeholder="Username" form="login">
                </p>
            -->
                <p class="elt">
                    <input type="password" name="password" placeholder="Password" form="login">
                    
                </p>
        
        
                <p class="elt">
                    <input type="submit" name="submit" value="Submit" form="login">
                </p>
                
                <p class="elt">
                    <!--<div class="g-signin2" data-onsuccess="onSignIn"></div>-->
                        <!-- <div class="g-signin2" data-width="315" data-height="30" data-longtitle="true"></div> -->
                        <a class="googleButton" href="/google/callback">Log In With Google</a>
                </p>
        
                </form>
            </div>

        </div>      
        
    </div>

</div>

<!-- Footer -->
<%- include('../partials/footer'); %>


</body>
</html>