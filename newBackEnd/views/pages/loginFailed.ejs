<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>

    <link rel="stylesheet" type="text/css" href="login.css">
</head>
<body>

<!-- Header -->
<%- include('../partials/header'); %>

<!-- main section -->
<div class="form">
    <h2 class="elt">Login</h2>
    <h3 style="color: white;">Login Failed. Email or password wrong.</h3>

    <form method="post" action="/loginAccount" id="login">

     <p class="elt">
        <input type="text" name="email" placeholder="Email" form="login">
    </p>

    <p class="elt">
        <input type="password" name="password" placeholder="Password" form="login">
    </p>

    <p class="elt">
        <input type="submit" name="submit" value="Submit" form="login">
        <!--<input type="reset" name="reset" value="Clear Form" form="login"> -->
    </p>
    <p class="elt">
        <!-- <div class="g-signin2" data-onsuccess="onSignIn"></div> -->
        <a style="padding: 10px; background-color: white; color: black;"href="/google/callback">Log In With Google</a>
    </p>

    </form>
</div>

<!-- Footer -->
<%- include('../partials/footer'); %>


</body>
</html>