<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
    <%- include('../partials/googleHead'); %>

    <link rel="stylesheet" type="text/css" href="signUp.css">
</head>
<body>

<!-- Header -->
<%- include('../partials/header'); %>

<!-- main section -->
<div class="form">
    <h2 class="elt">Sign Up</h2> 

    <form method="post" action="/createAccount" id="signUp">

     <p class="elt">
        <input type="text" name="email" placeholder="Email" form="signUp">
    </p>

    <p class="elt">
        <input type="text" name="username" placeholder="Username" form="signUp">
    </p>

    <p class="elt">
        <input type="password" name="password" placeholder="Password" form="signUp">
    </p>

    <p class="elt">
        <input type="password" name="confirmPass" placeholder="Confirm Password" form="signUp">
    </p>

    <p class="elt">
        <input type="submit" name="submit" value="Submit" form="signUp">
        <!--<input type="reset" name="reset" value="Clear Form" form="signUp"> -->
    </p>

    <p class="elt">
        <!-- <div class="g-signin2" data-onsuccess="onSignIn"></div> -->
        <a style="padding: 10px; background-color: white; color: black;"href="/google/callback">Log In With Google</a>
    </p>
    </form>
</div>

<!-- Footer -->
<%- include('../partials/footer'); %>


</body>
</html>